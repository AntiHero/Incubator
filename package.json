{
  "name": "incubator-nest",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "MIT",
  "scripts": {
    "vercel-build": "nest build",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "yarn migration:run && nest start",
    "start:dev": "cross-env DEV_MODE=true nest start --watch",
    "start:debug": "cross-env DEV_MODE=true nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:debug:e2e": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register ./node_modules/jest/bin/jest --config ./test/jest-e2e.json",
    "test:e2e": "cross-env TEST=true jest --config ./test/jest-e2e.json",
    "postinstall": "husky install",
    "typeorm": "typeorm-ts-node-esm -d db/data-source.ts",
    "migration:generate": "yarn typeorm migration:generate",
    "migration:create": "typeorm migration:create ./migrations/EmptyMigration",
    "migration:run": "yarn typeorm migration:run",
    "migration:revert": "yarn typeorm migration:revert",
    "db:drop": "yarn typeorm schema:drop",
    "schema:sync": "yarn typeorm schema:sync"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.279.0",
    "@google-cloud/storage": "^6.9.3",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/mapped-types": "^1.2.0",
    "@nestjs/mongoose": "^9.2.1",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/typeorm": "^9.0.1",
    "@typegoose/typegoose": "^9.12.1",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/imap": "^0.8.35",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/module-alias": "^2.0.1",
    "@types/mongoose": "^5.11.97",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.0.0",
    "@types/nodemailer": "^6.4.6",
    "@types/passport-jwt": "^3.0.7",
    "@types/sharp": "^0.31.1",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "husky": "^8.0.2",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "mongoose": "^6.7.2",
    "multer": "^1.4.5-lts.1",
    "nestjs-typegoose": "^7.1.38",
    "nodemailer": "^6.8.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "sharp": "^0.31.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typeorm": "0.3.11",
    "typescript": "^4.9.5",
    "uuidv4": "^6.2.13"
  },
  "devDependencies": {
    "@google-cloud/local-auth": "2.1.0",
    "@nestjs/testing": "^9.0.0",
    "@types/jest": "28.1.8",
    "@types/sinon": "^10.0.13",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "cookie-session": "^2.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "googleapis": "105",
    "imap": "^0.8.19",
    "jest": "28.1.3",
    "lint-staged": "^13.0.3",
    "pinst": "^3.0.0",
    "prettier": "^2.8.3",
    "sinon": "^15.0.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.8"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix --cache"
    ],
    "*.(ts|md|json)": [
      "prettier --write --cache"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^root/(.*)$": "<rootDir>/$1"
    }
  },
  "_moduleAliases": {
    "root": "src"
  },
  "engines": {
    "node": "18.x"
  }
}
